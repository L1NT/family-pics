[DEFAULT]
# root directory of the webserver
WEB_ROOT = /var/www

# root directory of the media & location of the thumbnail cache, relative
# to the web server root
PIC_URL = /pics
CACHE = /pics/_fam_pics_cache

# the extensions to return as valid urls
EXTENSIONS = ['jpg', 'JPG', 'MPG', 'MOV', '3gp']
